{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "import/extensions":"off",
    "func-names":"off",
    "import/no-unresolved":"off",
    "max-len":"off",
    "class-methods-use-this":"off",
    "no-param-reassign":"off",
    "no-console":"off"
  }
}